{
  "manifest_version": 3,
  "name": "Nested JSON Viewer",
  "description": "Pretty-print JSON and recursively open stringified JSON",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["clipboardWrite", "tabs"],
  "icons": {
    "128": "icon.png"
  }
}
